<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="anchor" prefHeight="175.0" prefWidth="230.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="20.0" layoutY="12.0" text="Name" />
      <TextField fx:id="setName" layoutX="15.0" layoutY="31.0" prefHeight="25.0" prefWidth="200.0" />
      <CheckBox fx:id="currentAsBase" layoutX="15.0" layoutY="71.0" mnemonicParsing="false" text="Use current set as base" />
      <Separator layoutY="107.0" prefHeight="0.0" prefWidth="230.0" />
      <Button fx:id="create" layoutX="15.0" layoutY="122.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="OK" onMouseClicked="#createNewSet"/>
      <Button fx:id="cancel" layoutX="134.0" layoutY="122.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="CANCEL" onMouseClicked="#closeWindow"/>
   </children>
</AnchorPane>
